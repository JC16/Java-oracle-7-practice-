package com.khh.app;

import java.util.Scanner;

public class TestNestedif {
    public static void main(String[] args)
    {
        //輸出文字
    	System.out.println("Please enter score(1~100):");
    	//建構Scanner物件與InputStream物件互動
    	Scanner scanner=new Scanner(System.in);
    	int scope=scanner.nextInt();
    	//巢狀判斷式
    	System.out.println(scope);
    if (scope>=90 && scope<=100)
    	{
    		System.out.println("A");
    	}else if(scope>=80 & scope<90)
    	{
    		System.out.println("B");
    	}else if(scope>=70 & scope<80)
    	{
    		System.out.println("C");
    	}else if(scope>=60 & scope<70)
    	{
    		System.out.println("D");
    	}else if(scope<60 & scope>=0)
    	{
    		System.out.println("F");
    	}else
    	{
    		System.out.println("Out of Range");
    	}
    }
}
