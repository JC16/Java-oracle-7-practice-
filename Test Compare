public class TestCompare {
	public static void main(String[] args)
	{
		int i1=100,i2=200; //固定長度 值型別
		boolean r1=i1==i2; //= assign == 相等
		System.out.println(r1);
		//類別是參考型別 s1區域變數stack "eric"物件 Heap堆積
		String s1="eric"; //字串類別 字串物件 = 將物件位址給s1
		String s2="eric"; 
		boolean r2=s1==s2;
		System.out.println(r2);
		//使用建構子方式建構物件
		String s3=new String("eric"); //字串類別 字串物件 = 將物件位址給s1
		String s4=new String("eric"); 
		boolean r3=s3==s4;
		System.out.println(r3);
		
	}

}
